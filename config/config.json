{
  "BASE_URL": "https://api.openweathermap.org/data/",
  "BATCH_SIZE": 100,
   "URL_VERSION": {
    "ONE_CALL_V": "3.0",
    "WEATHER_V": "2.5"
  },
  "END_POINTS": {
    "WEATHER": "/weather",
    "ONE_CALL" :"/onecall"
  },

  "S3": {
    "SKYCAST-BUCKET":{
      "NAME":"skycast-weather-report",
      "DISCLAIMER FOR KEYS": "Maintain the index order of the KEYS list",
      "KEYS": ["reference/dim_city.parquet","staging/raw_weather_data.parquet","staging/flattened_weather_data.parquet"]
    }
  },
  "SLEEP": {
    "MAX_TRIES": 3,
    "RETRY_DELAY": 5,
    "BATCH_DELAY": 60
  },
  "COLUMNS": {
    "FETCHED_AT": "fetched_at",
    "FLATTENED_COLS": {
      "BASE": "string",
      "VISIBILITY": "Int64",
      "DT": "datetime[ns,UTC]",
      "TIMEZONE": "Int64",
      "ID": "Int64",
      "NAME": "string",
      "COD": "Int64",
      "FETCHED_AT": "datetime64[ns]",
      "RAIN": "float64",
      "COORD_LAT": "float64",
      "COORD_LON": "float64",
      "MAIN_FEELS_LIKE": "float64",
      "MAIN_GRND_LEVEL": "Int64",
      "MAIN_HUMIDITY": "Int64",
      "MAIN_PRESSURE": "Int64",
      "MAIN_SEA_LEVEL": "Int64",
      "MAIN_TEMP": "float64",
      "MAIN_TEMP_MAX": "float64",
      "MAIN_TEMP_MIN": "float64",
      "WIND_DEG": "Int64",
      "WIND_GUST": "float64",
      "WIND_SPEED": "float64",
      "CLOUDS_ALL": "Int64",
      "SYS_COUNTRY": "string",
      "SYS_SUNRISE": "datetime[ns,UTC]",
      "SYS_SUNSET": "datetime[ns,UTC]",
      "RAIN_1H": "float64",
      "WEATHER_DESCRIPTION": "string",
      "WEATHER_ICON": "string",
      "WEATHER_ID": "Int64",
      "WEATHER_MAIN": "string"
    }

  }
}